{
  "extends": "fullstack",
  "root": true,
  "overrides": [
    {
      "files": ["*.test.js", "*.spec.js"],
      "rules": {
        "no-unused-expressions": "off"
      }
    }
  ],
  "rules": {
    "no-console": "warn",
    "no-unused-vars": "warn"
  },
  "settings": {
    "react": {
      "version": "detect" // Detect react version
    }
  }
}
